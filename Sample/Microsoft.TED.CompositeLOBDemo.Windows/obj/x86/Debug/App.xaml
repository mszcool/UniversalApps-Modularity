<micro:CaliburnApplication
    x:Class="Microsoft.TED.CompositeLOBDemo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Microsoft.TED.CompositeLOBDemo"
    xmlns:micro="using:Caliburn.Micro">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <ai:ApplicationInsightsModule x:Key="ApplicationInsightsModule" xmlns:ai="using:Microsoft.ApplicationInsights.Telemetry.WindowsStore" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</micro:CaliburnApplication>

